  "sections": {
    "template": {
      "type": "template-product",
      "settings": {
      }
    },
    "contact_form": {
      "type": "contact-form",
      "settings": {
       
      }
    }
  },
  "order": [
    "template",
    "contact_form"
  ]
} {% endcomment %}
