 {% case media.media_type %}
    {% when 'image' %}
        <img src="{{ media | img_url: 'large' }}" alt="{{ media.preview_image.alt }}" class="w-full h-full object-cover">
    {%  when 'external_video' %} 
      {{ media | external_video_tag: class: 'w-full h-80 object-cover' }}
    {% else %}
      <h1>nothing found</h1>
        
{% endcase %}
